<div class="pull-left">
  <div class="btn-group">
    <button class="btn btn-default btn-lg">
      <i class="fa fa-bars"></i>
    </button>
  </div>
</div>

<div class="pull-right" id="ol-editor-actions">
  <div class="btn-group">
    <button class="btn btn-default btn-lg action-undo">
      <i class="fa fa-undo"></i>
    </button>
    <button class="btn btn-default btn-lg action-copy">
      <i class="fa fa-copy"></i>
    </button>
    <button class="btn btn-default btn-lg action-skip">
      <i class="fa fa-forward"></i>
    </button>
    <button class="btn btn-default btn-lg action-unapprove">
      <i class="fa fa-thumbs-down"></i>
    </button>
    <button class="btn btn-default btn-lg action-approve" disabled="">
      <i class="fa fa-thumbs-up"></i>
    </button>
    <button class="btn btn-success btn-lg btn-wide action-save">
      Save
    </button>
  </div>
</div>
