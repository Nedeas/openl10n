<div class="x-editor--header">
  <div class="pull-right x-editor--locales-chooser form-inline">
    <span id="source-chooser"></span>

    <span class="fa fa-chevron-right"></span>

    <span id="target-chooser"></span>
  </div>

  <div class="x-editor--resources-list" id="resources-list"></div>
</div>

<div class="x-editor--subheader">
  <div class="x-editor--filters" id="filters"></div>

  <div class="x-editor--stats pull-left" id="stats"></div>

  <div class="x-editor--actions pull-right" id="actionbar"></div>
</div>

<div class="inner">
  <div class="x-editor--translations-selection">
    <div class="x-editor--translations-list js-scrollable" id="translations-list">

      <div class="spinner spinner-bounce">
        <div class="bounce1"></div>
        <div class="bounce2"></div>
        <div class="bounce3"></div>
      </div>
    </div>
  </div>

  <div class="x-editor--translation-edit js-scrollable" id="translations-edit"></div>
</div>
