parameters:
    # General
    locale: en
    secret: this_token_is_not_so_secret_change_it

    # Database
    database_driver:   pdo_mysql
    database_host:     127.0.0.1
    database_port:     ~
    database_name:     openl10n
    database_user:     root
    database_password: ~

    # Web-app
    app_base_url: cdn.openl10n.io/app
    app_version_format: %%2$s/%%1$s
